<%# overridden in order to add maps %>

<div class="navbar navbar-inverse">
  <div class="container">
      <ul class="nav navbar-nav">
        <li class="<%= active?(home_path) %>">
          <%= link_to t("home.title", default: "Home"), home_path %>
        </li>
        <li class="<%= active?(/^#{config.relative_url_root}\/browse/,
          request.path) %>">
          <%= link_to t("browse.title", default: "Browse"), browse_path %>
        </li>
        <li class="<%= active?(search_path) %>">
          <%= link_to t("search.title", default: "Search"), search_path, rel: "search" %>
        </li>
        <li class="<%= active?(about_path) %>">
          <%= link_to t("about.title", default: "About"), about_path %>
        </li>
        <li class="<%= active?(map_path) %>">
          <%= link_to t("map.title", default: "Map"), map_path %>
        </li>
      </ul>
  </div>
</div><!-- /navbar -->
